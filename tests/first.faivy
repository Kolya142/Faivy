fn001 :: (a s32) {
    printf("Hello, %d!\n", a+42);
}

fn003 :: (a s32) {
    while a {
        printf("%d\n", a);
        #cpp "--a;";
    }
}

jk :: () {
    return "Hello, from jk()";
}

fn002 :: (a bool) {
    printf("Programming, cup of tea No.%d!\n", #cpp "mtemp(69-42)" + 10);
    #run print("Hi, from comptime!\n");
    if a printf("%d!\n`%s`\n", #run 640*2+820, jk());
    else printf("a=false; skipping\n");
}

main :: () {
    printf("__________\nfn001(69);\n");
    fn001(69);
    printf("__________\nfn002(true);\n");
    fn002(true);
    printf("__________\nfn002(false);\n");
    fn002(false);
    printf("__________\nfn003(5);\n");
    fn003(5);
}